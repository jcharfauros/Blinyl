@model IEnumerable<Blinyl.Models.WishListItem>

@{
    ViewBag.Title = "Index";
}


<div class="text-center">
    <h2>Your BLINYL Wishlist(s)</h2>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
</div>

<table table class="content-table center" style="margin-left:auto;margin-right:auto;">
    <thead>
        <tr style="font-size: 2em; font-family:'Varela Round', sans-serif; color: #6a7cff; text-shadow: 2px 2px #DAF7A6">
            <th>
                @Html.DisplayNameFor(model => model.WishId)
            </th>
            <!--<th>
                Html.DisplayNameFor(model => model.OwnerId)
            </th> -->
            <th>
                @Html.DisplayNameFor(model => model.WishListTitle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedUtc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModifiedUtc)
            </th>
            <th> <img src="~/Content/Images1/Blinyl_smcl.png" /></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.WishId)
                </td>
                <!--<td>
                    Html.DisplayFor(modelItem => item.OwnerId)
                </td> -->
                <td>
                    @Html.DisplayFor(modelItem => item.WishListTitle)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CreatedUtc)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ModifiedUtc)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.WishId }) |
                    @Html.ActionLink("Details", "Details", new { id = item.WishId }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.WishId })
                </td>
            </tr>
        }
    </tbody>

</table>
